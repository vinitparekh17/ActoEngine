{
    "Logging": {
        "LogHeaders": {
            "Enabled": true
        },
        "LogBody": {
            "Enabled": true
        },
        "LogResponseBody": {
            "Enabled": true
        }
    },
    "AllowedHosts": "localhost;127.0.0.1",
    "DatabaseSettings": {
        "CommandTimeout": 30,
        "MaxRetryCount": 3,
        "MaxPoolSize": 100
    },
    "Jwt": {
        "Issuer": "MyApp",
        "Audience": "MyAppUsers",
        "SecretKey": "ADD_SECURE_SECRET_KEY_HERE",
        "ExpirationMinutes": 60,
        "ValidateIssuer": true,
        "ValidateAudience": true,
        "ValidateLifetime": true,
        "ValidateIssuerSigningKey": true
    },
    "DatabaseSeeding": {
        "Enabled": true,
        "SeedDevelopmentData": false,
        "DefaultPasswords": {
            "AdminUser": "CHANGE_ME_DEV_ONLY"
        },
        "AdminUser": {
            "Username": "admin",
            "FullName": "System Administrator",
            "Role": "Admin"
        }
    }
}