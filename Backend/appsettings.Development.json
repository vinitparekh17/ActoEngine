{
    "Logging": {
        "LogHeaders": {
            "Enabled": true
        },
        "LogBody": {
            "Enabled": true
        },
        "LogResponseBody": {
            "Enabled": true
        }
    },
    "AllowedHosts": "localhost;127.0.0.1",
    "Cors": {
        "AllowedOrigins": [
            "http://localhost:5173",
            "http://localhost:3000"
        ]
    },
    "DatabaseSettings": {
        "CommandTimeout": 30,
        "MaxRetryCount": 3,
        "MaxPoolSize": 100
    },
    "DatabaseSeeding": {
        "Enabled": true,
        "SeedDevelopmentData": false,
        "DefaultPasswords": {
            "AdminUser": "CHANGE_ME_DEV_ONLY"
        },
        "AdminUser": {
            "Username": "admin",
            "FullName": "System Administrator",
            "Role": "Admin"
        }
    }
}